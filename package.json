{
    "name": "better-lisp-syntax",
    "displayName": "Better Lisp Syntax",
    "description": "",
    "version": "0.0.3",
    "icon": "icon.png",
    "scripts": {},
    "keywords": [
        "syntax",
        "textmate",
        "highlighting",
        "coloring",
        "color"
    ],
    "repository": "git@github.com:jeff-hykin/better-lisp-syntax.git",
    "engines": {
        "vscode": "^1.0.0"
    },
    "author": {
        "name": "Jeff Hykin"
    },
    "publisher": "jeff-hykin",
    "contributes": {
        "languages": [
			{
				"id": "lisp",
				"aliases": [
					"Lisp",
					"lisp"
				],
				"extensions": [
					".lisp",
					".cl",
					".asd",
					".ros",
					".lsp",
					".el",
					".fnl"
				],
				"configuration": "./main/configuration.json"
			}
		],
		"grammars": [
			{
				"language": "lisp",
				"scopeName": "source.lisp",
				"path": "./syntaxes/Lisp.tmLanguage"
			},
			{
				"scopeName": "markdown.lisp.codeblock",
				"path": "./autogenerated/lisp.tmLanguage.json",
				"injectTo": [
					"text.html.markdown"
				],
				"embeddedLanguages": {
					"meta.embedded.block.superjs": "lisp"
				}
			}
		]
    },
    "devDependencies": {
        "chalk": "^2.4.2",
        "dictionary-en-us": "^2.1.1",
        "lodash": "^4.17.19",
        "nspell": "^2.1.2",
        "oniguruma": "^7.2.0",
        "textmate-bailout": "^1.1.0",
        "vsce": "^1.99.0",
        "yargs": "^17.5.1",
        "ovsx": "^0.5.1",
        "textmate-tester": "^1.1.9"
    }
}
